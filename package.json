{
  "name": "nextjs-with-typescript",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "post-update": "echo \"codesandbox preview only, need an update\" && npm upgrade --latest",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:ct": "playwright test -c playwright-ct.config.ts",
    "test:all": "run-p test:run test:e2e test:ct",
    "typecheck": "tsc --project tsconfig.json --noEmit --preserveWatchOutput",
    "prettier": "prettier . --ignore-path .gitignore --loglevel=silent --check",
    "check": "run-p typecheck lint prettier",
    "format": "npm run prettier -- --write",
    "fix": "run-p lint format",
    "commitlint": "npx commitlint --edit --verbose",
    "commitlint:last": "npx commitlint --from HEAD~1 --to HEAD --verbose"
  },
  "dependencies": {
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@playwright/experimental-ct-react": "^1.23.4",
    "@playwright/test": "^1.23.4",
    "@types/node": "latest",
    "@types/react": "latest",
    "@vitejs/plugin-react": "^2.0.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^8.5.0",
    "jsdom": "^20.0.0",
    "lefthook": "^1.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vitest": "^0.18.1"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=16"
  }
}
