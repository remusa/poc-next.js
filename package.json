{
  "name": "poc-nextjs",
  "version": "5.0.0",
  "private": true,
  "homepage": "https://poc-nextjs-rms7.vercel.app/",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "post-update": "echo \"codesandbox preview only, need an update\" && npm upgrade --latest",
    "setup": "npx rimraf node_modules/ && npx rimraf .next/ && npm i",
    "test": "npx vitest",
    "test:run": "npx vitest run",
    "test:coverage": "npx vitest run --coverage",
    "test:ct": "playwright test -c playwright-ct.config.ts",
    "test:e2e": "playwright test",
    "test:all": "run-p test:run test:ct test:e2e",
    "test:debug": "cross-env PWDEBUG=1 playwright test -c playwright-debug.config.ts",
    "test:debug:api": "cross-env PWDEBUG=console DEBUG=pw:api playwright test -c playwright-debug.config.ts --debug",
    "test:debug:server": "cross-env PWDEBUG=console DEBUG=pw:webserver playwright test -c playwright-debug.config.ts  --debug",
    "typecheck": "tsc --project tsconfig.json --noEmit --preserveWatchOutput",
    "prettier": "prettier . --loglevel=silent --check",
    "check": "run-p typecheck lint prettier",
    "format": "npm run prettier -- --write",
    "lint:fix": "npm run lint -- --fix",
    "fix": "run-p lint:fix format",
    "commitlint": "npx commitlint --edit --verbose",
    "commitlint:last": "npx commitlint --from HEAD~1 --to HEAD --verbose",
    "deploy:preview": "vercel deploy",
    "deploy": "vercel deploy --prod"
  },
  "dependencies": {
    "@emotion/cache": "11.10.3",
    "@emotion/react": "11.10.4",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.4",
    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.6",
    "next": "12.3.1",
    "phoenix": "1.6.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "2.1.14"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@faker-js/faker": "7.5.0",
    "@mswjs/data": "0.10.2",
    "@playwright/experimental-ct-react": "1.26.0",
    "@playwright/test": "1.26.0",
    "@types/node": "18.6.1",
    "@types/phoenix": "1.5.4",
    "@types/react": "18.0.21",
    "@vitejs/plugin-react": "2.1.0",
    "cross-env": "7.0.3",
    "dotenv": "16.0.2",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "jsdom": "20.0.0",
    "lefthook": "1.1.1",
    "msw": "0.47.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "typescript": "4.8.3",
    "vite": "3.1.3",
    "vitest": "0.23.4"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=16"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
